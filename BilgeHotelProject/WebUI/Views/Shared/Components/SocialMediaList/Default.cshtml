@model VMSocialMedia

@{
    if (Model != null && Model.Twitter != null)
    {
        <a href="@Model.Twitter" class="twitter"><i class="icofont-twitter"></i></a>
    }
    if (Model != null && Model.Facebook != null)
    {
        <a href="@Model.Facebook" class="facebook"><i class="icofont-facebook"></i></a>
    }
    if (Model != null && Model.Instagram != null)
    {
        <a href="@Model.Instagram" class="instagram"><i class="icofont-instagram"></i></a>
    }
}


